<!--
 *CSOD DETAILS
-->
    <xsd:complexType name="CSOD.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>SOD details consists of
                               a) SOD Key Object
                               b) Prosecution Department Code (v1.1)
                               c) Status
                               d) Effective Date
                               e) Short Description in English
                               f) Short Description in Chinese
                               g) Long Description in English
                               h) Long Description in Chinese
                               i) Law in English
                               j) Law in Chinese
                               k) Attachment Indicator
                               l) Hearing Weight
                               m) Penalty Weight
                               n) Fixed Penalty Fine Code
                               o) Fixed Penalty Costs Code
                               p) Time Bar Offence Period in Months
                               q) Time Bar Discover Period in Months
                               r) Apply to Fixed Penalty Summons Indicator
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="SODKey" type="SODKey.V1.0.CT"/>
            <xsd:element name="ProsecutionDepartmentCode" type="ProsecutionDepartmentCode.V1.1.CT"/>
            <xsd:element name="Status" type="Status.V1.0.CT"/>
            <xsd:element name="EffectiveDate" type="cct:Date.CT"/>
            <xsd:element name="ShortDescription" type="ShortDescription.V1.0.CT"/>
            <xsd:element name="ShortDescriptionChinese" type="ShortDescriptionChinese.V1.0.CT"/>
            <xsd:element name="LongDescription" type="OffenceDescription.V1.0.CT"/>
            <xsd:element name="LongDescriptionChinese" type="OffenceDescriptionChinese.V1.0.CT"/>
            <xsd:element name="Law" type="Law.V1.0.CT"/>
            <xsd:element name="LawChinese" type="LawChinese.V1.0.CT"/>
            <xsd:element name="AttachmentIndicator" type="FieldIndicator.V1.0.CT" minOccurs="0"/>
            <xsd:element name="HearingWeight" type="HearingWeight.V1.0.CT" minOccurs="0"/>
            <xsd:element name="PenaltyWeight" type="PenaltyWeight.V1.0.CT" minOccurs="0"/>
            <xsd:element name="FixedPenaltyFineCode" type="FixedPenaltyFineCode.V1.0.CT" minOccurs="0"/>
            <xsd:element name="FixedPenaltyCostsCode" type="FixedPenaltyCostsCode.V1.0.CT" minOccurs="0"/>
            <xsd:element name="TimeBarOffence" type="TimeBarOffence.V1.0.CT" minOccurs="0"/>
            <xsd:element name="TimeBarDiscover" type="TimeBarDiscover.V1.0.CT" minOccurs="0"/>
            <xsd:element name="ApplyToFixedPenaltySummons" type="FieldIndicator.V1.0.CT" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

