            <xsd:restriction base="cct:Code.CT">
				<xsd:enumeration value="1"/>
				<xsd:enumeration value="2"/>
				<xsd:enumeration value="3"/>
				<xsd:enumeration value="4"/>				
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="DICType.V1.1.CT">
        <xsd:annotation>
            <xsd:documentation>DIC Type
		1 - DIC to be completed within 3 months
        	2 - DIC to be completed within last 3 months of the disqualification order
            	3 - DIC to be completed within the period specified by court
		4 - DIC to be completed but without specified period
		5 - DIC to be completed within 3 months after imprisonment/detention
		6 - DIC to be completed with detention period not yet ascertainable
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
				<xsd:enumeration value="1"/>
				<xsd:enumeration value="2"/>
				<xsd:enumeration value="3"/>
				<xsd:enumeration value="4"/>				
				<xsd:enumeration value="5"/>				
				<xsd:enumeration value="6"/>				
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="DqrEntitlementInd.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>DQR Entitlement Indicator
		A - All entitlements
        	E - All entitlements with Exception
            	I - Entitlements Include
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
				<xsd:enumeration value="A"/>
				<xsd:enumeration value="E"/>
				<xsd:enumeration value="I"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="DqrEntitlementList.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>DQR Entitlement List</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Text.CT">
                   <xsd:maxLength value="100"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
   
    <xsd:complexType name="LinkSerialNumber.V1.1.CT">
        <xsd:annotation>
            <xsd:documentation>order link serial number version 1.1</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Count.CT">
            	<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="999"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="OrderInternalNumber.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>order internal number for a specific hearing</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Numeric.CT">
					<xsd:totalDigits value="3"/>
               <xsd:fractionDigits value="0"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="OrderCode.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>order code, coutcome.outcome</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
					<xsd:maxLength value="3"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


    <xsd:complexType name="Year.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>number of years in imprisonment/disqualification order</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Numeric.CT">
					<xsd:totalDigits value="4"/>
               <xsd:fractionDigits value="0"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


   <xsd:complexType name="Month.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>number of months in imprisonment/disqualification order</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Numeric.CT">
					<xsd:totalDigits value="5"/>
               <xsd:fractionDigits value="0"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


   <xsd:complexType name="Week.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>number of weeks in imprisonment/disqualification order</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Numeric.CT">
					<xsd:totalDigits value="5"/>
               <xsd:fractionDigits value="0"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


   <xsd:complexType name="Day.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>number of days in imprisonment/disqualification order</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Numeric.CT">
					<xsd:totalDigits value="5"/>
               <xsd:fractionDigits value="0"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


   <xsd:complexType name="Hour.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>number of hours in imprisonment/disqualification order</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Numeric.CT">
					<xsd:totalDigits value="5"/>
               <xsd:fractionDigits value="0"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


    <xsd:complexType name="CurrencyCode.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>currency code, cmcode.code_type=CCY</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
					<xsd:maxLength value="3"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


    <xsd:complexType name="Amount.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>penalty amount</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Numeric.CT">
					<xsd:totalDigits value="10"/>
               <xsd:fractionDigits value="2"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


<!--
 *APPEAL DETAILS
-->
    <xsd:complexType name="Appeal.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>appeal details consists of 
                               a) Case Number Object
                               b) Appeal Internal Number
                               c) Appeal Register Serial Number
                               d) Filing Date
                               e) Applied Party 
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="CaseNumber" type="CaseNumber.V1.0.CT"/>
            <xsd:element name="AppealInternalNumber" type="AppealInternalNumber.V1.0.CT"/>
            <xsd:element name="AppealRegisterNumber" type="AppealRegisterNumber.V1.0.CT" minOccurs="0"/>
            <xsd:element name="FilingDate" type="cct:Date.CT"/>
            <xsd:element name="AppliedParty" type="AppealAppliedParty.V1.0.CT"/>  
        </xsd:sequence>
    </xsd:complexType>

  
    <xsd:complexType name="AppealInternalNumber.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>appeal internal number for a specific case</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Numeric.CT">
					<xsd:totalDigits value="3"/>
               <xsd:fractionDigits value="0"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


    <xsd:complexType name="AppealRegisterNumber.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>appeal register serial number</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Text.CT">
					<xsd:maxLength value="13"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


    <xsd:complexType name="AppealAppliedParty.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>appeal applied party, DEF-Defendant, PRO-Prosecution
                               T - 3rd Party
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
					<xsd:enumeration value="DEF"/>
               <xsd:enumeration value="PRO"/>
					<xsd:enumeration value="T"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


<!--
 *REVIEW DETAILS
-->
    <xsd:complexType name="Review.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>review details consists of 
                               a) Case Number Object
                               b) Review Internal Number
                               c) Filing Date
                               d) Applied Party
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="CaseNumber" type="CaseNumber.V1.0.CT"/>
            <xsd:element name="ReviewwInternalNumber" type="ReviewInternalNumber.V1.0.CT"/>
            <xsd:element name="FilingDate" type="cct:Date.CT"/>
            <xsd:element name="AppliedParty" type="ReviewAppliedParty.V1.0.CT"/>  
        </xsd:sequence>
    </xsd:complexType>

  
    <xsd:complexType name="ReviewInternalNumber.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>review internal number for a specific case</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Numeric.CT">
					<xsd:totalDigits value="3"/>
               <xsd:fractionDigits value="0"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


   <xsd:complexType name="ReviewAppliedParty.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>review applied party, DEF-Defendant, PRO-Prosecution
                               T - 3rd Party, M - Magistrate
            </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
					<xsd:enumeration value="DEF"/>
               <xsd:enumeration value="PRO"/>
					<xsd:enumeration value="T"/>
					<xsd:enumeration value="M"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


<!--
 *ERROR MESSAGE
-->
    <xsd:complexType name="ErrorMessage.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>error message</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="ProsecutionDepartmentCode" type="ProsecutionDepartmentCode.V1.1.CT"/>
            <xsd:element name="DepartmentReferenceNumber" type="DepartmentReferenceNumber.V1.0.CT"/>
            <xsd:element name="ErrorDetails" type="ErrorDetails.V1.0.CT" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    

    <xsd:complexType name="ErrorDetails.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>error details</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="ErrorCode" type="cct:Code.CT" minOccurs="0"/>
            <xsd:element name="ErrorMessage" type="cct:Text.CT"/>
        </xsd:sequence>
    </xsd:complexType>

<!--
 *DURATION
-->
    <xsd:complexType name="Duration.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>Duration consists of
                               a) Minute
                               b) Second
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="Minute" type="Minute.V1.1.CT"/>
            <xsd:element name="Second" type="Second.V1.0.CT"/>
        </xsd:sequence>
    </xsd:complexType>   
	
    <xsd:complexType name="Minute.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>minute for duration</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Numeric.CT">
				<xsd:totalDigits value="2"/>
				<xsd:fractionDigits value="0"/>
				<xsd:minInclusive value="0"/>
				<xsd:maxInclusive value="59"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="Minute.V1.1.CT">
        <xsd:annotation>
            <xsd:documentation>minute for duration</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Numeric.CT">
				<xsd:totalDigits value="2"/>
				<xsd:fractionDigits value="0"/>
				<xsd:minInclusive value="0"/>
				<xsd:maxInclusive value="99"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
	
    <xsd:complexType name="Second.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>second for duration</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Numeric.CT">
				<xsd:totalDigits value="2"/>
				<xsd:fractionDigits value="0"/>
				<xsd:minInclusive value="0"/>				
				<xsd:maxInclusive value="59"/>			   
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>

<!--
 *Vehicle Registration Mark
-->	
    <xsd:complexType name="VehicleRegistrationMark.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Vehicle Registration Mark</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
			<xsd:maxLength value="8"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	
	<xsd:complexType name="VehicleRegistrationMarkDigit.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Vehicle Registration Mark Digit</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
			<xsd:maxLength value="1"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

<!--
 *PAYMENT ORDER DETAILS VERSION 1.0
-->
    <xsd:complexType name="PaymentOrder.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>payment order details version 1.0 consists of
								a) Payment Type
								b) Payment Amount
								c) Payment Currency
								d) Payment Parameter

            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>                       
            <xsd:element name="Type" type="PaymentType.V1.0.CT"/>
            <xsd:element name="Amount" type="Amount.V1.0.CT"/>
            <xsd:element name="Currency" type="CurrencyCode.V1.0.CT"/>
            <xsd:element name="Parameter" type="PaymentParameter.V1.0.CT" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="PaymentType.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Payment Type (To be updated)
	            	'FIN': Fine
	            	'AMD': Compensation
	            	'COS': Court Cost
                        '': Daily Fine
                        'MR1': Business Registration Fee
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:maxLength value="3"/>
				<xsd:enumeration value="FIN"/>
				<xsd:enumeration value="AMD"/>
				<xsd:enumeration value="COS"/>
				<xsd:enumeration value="MR1"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
    
    <xsd:complexType name="PaymentParameter.V1.0.CT">
            <xsd:annotation>
                    <xsd:documentation>Payment parameter</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleContent>
                    <xsd:restriction base="cct:Text.CT">
                            <xsd:maxLength value="30"/>
                    </xsd:restriction>
            </xsd:simpleContent>
    </xsd:complexType>
	
 </xsd:schema>
