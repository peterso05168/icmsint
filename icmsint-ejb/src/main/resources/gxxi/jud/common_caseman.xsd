<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema elementFormDefault="qualified" 
            attributeFormDefault="unqualified" 
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:cct="CCT"
            xmlns:hdc="HDC">

	<xsd:include schemaLocation="common_jud.xsd"/>
	<xsd:import namespace="CCT" schemaLocation="../ref/gov/cct/cct.xsd"/>
	<xsd:import namespace="HDC" schemaLocation="../ref/if/hksarg_department_code_v1_0.xsd"/>

<!--
 *ADDRESS DETAILS FOR CASEMAN
-->
    <xsd:complexType name="Address.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>address details consists of 
                               a) Address territory
                                  1 - HK
                                  2 - KLN
                                  3 - NT
                                  4 - Outlying Islands
                                  5 - Others
                               b) Address in English
                               c) Address in Chinese
                               d) District code
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="AddressTerritory" type="AddressTerritory.V1.0.CT" minOccurs="0"/>
            <xsd:element name="EnglishAddress" type="EnglishAddress.V1.0.CT" minOccurs="0"/>
            <xsd:element name="ChineseAddress" type="ChineseAddress.V1.0.CT" minOccurs="0"/>
            <xsd:element name="DistrictCode" type="DistrictCode.V1.0.CT" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

<!--
 *ADDRESS DETAILS FOR CASEMAN VERSION 1.1
-->
    <xsd:complexType name="Address.V1.1.CT">
        <xsd:annotation>
            <xsd:documentation>address details consists of 
                               a) Address territory
                                  1 - HK
                                  2 - KLN
                                  3 - NT
                                  4 - Outlying Islands
                                  5 - Others
                               b) Address in English (v1.1)
                               c) Address in Chinese (v1.1)
                               d) District code
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="AddressTerritory" type="AddressTerritory.V1.0.CT" minOccurs="0"/>
            <xsd:element name="EnglishAddress" type="EnglishAddress.V1.1.CT" minOccurs="0"/>
            <xsd:element name="ChineseAddress" type="ChineseAddress.V1.1.CT" minOccurs="0"/>
            <xsd:element name="DistrictCode" type="DistrictCode.V1.0.CT" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

<!--
 *ADDRESS DETAILS VERSION 1.2
-->
    <xsd:complexType name="Address.V1.2.CT">
        <xsd:annotation>
            <xsd:documentation>address details consists of 
                               a) Address territory
                                  1 - HK
                                  2 - KLN
                                  3 - NT
                                  4 - Outlying Islands
                                  5 - Others
                               b) Address in English (v1.2)
                               c) Address in Chinese (v1.2)
                               d) District code
                               e) Overseas Indicator
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="AddressTerritory" type="AddressTerritory.V1.0.CT" minOccurs="0"/>
            <xsd:element name="EnglishAddress" type="EnglishAddress.V1.2.CT" minOccurs="0"/>
            <xsd:element name="ChineseAddress" type="ChineseAddress.V1.2.CT" minOccurs="0"/>
            <xsd:element name="DistrictCode" type="DistrictCode.V1.0.CT" minOccurs="0"/>
            <xsd:element name="OverseasIndicator" type="FieldIndicator.V1.0.CT" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AddressTerritory.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>address territory code</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
					<xsd:length value="1"/>
               <xsd:pattern value="[1-5]{1}"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


    <xsd:complexType name="EnglishAddress.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>English address lines</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="AddressLine" type="AddressLine.V1.0.CT" maxOccurs="3"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="EnglishAddress.V1.1.CT">
        <xsd:annotation>
            <xsd:documentation>English address lines version 1.1</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="EnglishAddressLine" type="EnglishAddressLine.V1.1.CT" maxOccurs="5"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="EnglishAddress.V1.2.CT">
        <xsd:annotation>
            <xsd:documentation>English address lines version 1.2</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="EnglishAddressLine" type="EnglishAddressLine.V1.1.CT" maxOccurs="3"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ChineseAddress.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>Chinese address lines</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="AddressLine" type="AddressLine.V1.0.CT" maxOccurs="3"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="ChineseAddress.V1.1.CT">
        <xsd:annotation>
            <xsd:documentation>Chinese address lines version 1.1</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="ChineseAddressLine" type="ChineseAddressLine.V1.1.CT" maxOccurs="5"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="ChineseAddress.V1.2.CT">
        <xsd:annotation>
            <xsd:documentation>Chinese address lines version 1.2</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="ChineseAddressLine" type="ChineseAddressLine.V1.1.CT" maxOccurs="3"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="AddressLine.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>Address line, carriage return and line feed chars NOT allowed</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Text.CT">
                   <xsd:maxLength value="60"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="EnglishAddressLine.V1.1.CT">
        <xsd:annotation>
            <xsd:documentation>English Address line version 1.1, carriage return and line feed chars NOT allowed</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Text.CT">
                   <xsd:maxLength value="60"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="ChineseAddressLine.V1.1.CT">
        <xsd:annotation>
            <xsd:documentation>Chinese Address line version 1.1, carriage return and line feed chars NOT allowed</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Text.CT">
                   <xsd:maxLength value="30"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="DistrictCode.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>district code</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
					<xsd:maxLength value="3"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


<!--
 *CASE DETAILS
-->
    <xsd:complexType name="Case.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>case details derived from CaseNumber object
                               found in common_jud.xsd, additional fields consist of 
                               a) Prosecution Department Code (v1.1)
                               b) Department Reference Number
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="CaseNumber.V1.0.CT">
                <xsd:sequence>
                    <xsd:element name="ProsecutionDepartmentCode" type="ProsecutionDepartmentCode.V1.1.CT"/>
                    <xsd:element name="DepartmentReferenceNumber" type="DepartmentReferenceNumber.V1.0.CT"/>
                </xsd:sequence>
            </xsd:extension>
       </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="ProsecutionDepartmentCode.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>prosecution department code</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="hdc:HKSARGDepartmentCode.V1.0.CT">
               <xsd:maxLength value="8"/>
               <xsd:pattern value="[A-Z]{1,8}"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="ProsecutionDepartmentCode.V1.1.CT">
        <xsd:annotation>
            <xsd:documentation>prosecution department code version 1.1</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="hdc:HKSARGDepartmentCode.V1.0.CT">
				<xsd:minLength value="1"/>
				<xsd:maxLength value="8"/>               
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>    

    <xsd:complexType name="DepartmentReferenceNumber.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>case reference number</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
				<xsd:minLength value="1"/>
				<xsd:maxLength value="30"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="ProsecutionDepartmentUnit.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>prosecution department unit</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
				<xsd:minLength value="1"/>
				<xsd:maxLength value="10"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="ProsecutionDepartmentStation.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>prosecution department station</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
				<xsd:minLength value="1"/>
				<xsd:maxLength value="10"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


<!--
 *DEFENDANT DETAILS
-->
    <xsd:complexType name="Defendant.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>defendant object derives from Party
                               object found in common_jud.xsd, additional
                               elements include :
                               a) Defendant Type 
                               b) Address Object
                               c) Interpret Language
                               d) Special Arrangement
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="Party.V1.0.CT">
                <xsd:sequence>
                    <xsd:element name="DefendantType" type="DefendantType.V1.0.CT"/>
                    <xsd:element name="Address" type="Address.V1.0.CT"/>
                    <xsd:element name="InterpretLanguage" type="Language.V1.0.CT" minOccurs="0"/>
                    <xsd:element name="SpecialArrangement" type="SpecialArrangement.V1.0.CT" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

<!--
 *DEFENDANT DETAILS VERSION 1.1
-->
    <xsd:complexType name="Defendant.V1.1.CT">
        <xsd:annotation>
            <xsd:documentation>defendant version 1.1 object derives from Party (V1.1)
                               object found in common_jud.xsd, additional
                               elements include :
                               a) Defendant Type
                               b) Address Object (V1.1)
                               c) Interpret Language (v1.1)
                               d) Defendant District Code
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="Party.V1.1.CT">
                <xsd:sequence>
                    <xsd:element name="DefendantType" type="DefendantType.V1.0.CT"/>
                    <xsd:element name="Address" type="Address.V1.1.CT"/>
                    <xsd:element name="InterpretLanguage" type="Language.V1.1.CT" minOccurs="0"/>
                    <xsd:element name="DefendantDistrictCode" type="BailiffDistrictCode.V1.0.CT"  minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

<!--
 *DEFENDANT DETAILS VERSION 1.2
-->
    <xsd:complexType name="Defendant.V1.2.CT">
        <xsd:annotation>
            <xsd:documentation>defendant version 1.2 object derives from Party (V1.1)
                               object found in common_jud.xsd, additional
                               elements include :
                               a) Defendant Type
                               b) Address Object (V1.1)
                               c) Interpret Language (v1.1)
                               d) Special Arrangement
                               e) Defendant District Code
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="Party.V1.1.CT">
                <xsd:sequence>
                    <xsd:element name="DefendantType" type="DefendantType.V1.0.CT"/>
                    <xsd:element name="Address" type="Address.V1.1.CT"/>
                    <xsd:element name="InterpretLanguage" type="Language.V1.1.CT" minOccurs="0"/>
                    <xsd:element name="SpecialArrangement" type="SpecialArrangement.V1.0.CT" minOccurs="0"/>
                    <xsd:element name="DefendantDistrictCode" type="BailiffDistrictCode.V1.0.CT"  minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
<!--
 *DEFENDANT DETAILS VERSION 1.3
-->
    <xsd:complexType name="Defendant.V1.3.CT">
        <xsd:annotation>
            <xsd:documentation>defendant version 1.3 object derives from Party (V1.2)
                               object found in common_jud.xsd, additional
                               elements include :
                               a) Defendant Type
                               b) Defendant Alias Name in English
                               c) Defendant Alias Name in Chinese
                               d) Address Object (V1.2)
                               e) Interpret Language (v1.1)
                               f) Special Arrangement
                               g) Defendant District Code
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="Party.V1.2.CT">
                <xsd:sequence>
                    <xsd:element name="DefendantType" type="DefendantType.V1.0.CT"/>
                    <xsd:element name="PartyEnglishAliasName" type="PartyEnglishAliasName.V1.0.CT" minOccurs="0"/>
                    <xsd:element name="PartyChineseAliasName" type="PartyChineseAliasName.V1.0.CT" minOccurs="0"/>
                    <xsd:element name="Address" type="Address.V1.2.CT"/>
                    <xsd:element name="InterpretLanguage" type="Language.V1.1.CT" minOccurs="0"/>
                    <xsd:element name="SpecialArrangement" type="SpecialArrangement.V1.0.CT" minOccurs="0"/>
                    <xsd:element name="DefendantDistrictCode" type="BailiffDistrictCode.V1.0.CT"  minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

    <xsd:complexType name="DefendantType.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>defendant type, A-Adult, J-Juvenile, C-Company</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
					 <xsd:maxLength value="1"/>
                <xsd:enumeration value="A"/>
                <xsd:enumeration value="C"/>
                <xsd:enumeration value="J"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>

	<xsd:complexType name="BailiffDistrictCode.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>bailiff district code</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
					<xsd:maxLength value="3"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="SpecialArrangement.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>special arrangement</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Text.CT">
					<xsd:maxLength value="100"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="PartyEnglishAliasName.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>party alias name in english</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Name.CT">
                <xsd:maxLength value="400"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:complexType name="PartyChineseAliasName.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>party alias name in chinese</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Name.CT">
                <xsd:maxLength value="60"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>


<!--
 *INFORMANT DETAILS
-->
    <xsd:complexType name="Informant.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>informant object derives from Party object found in common_jud.xsd</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="Party.V1.0.CT">
            </xsd:extension>
        </xsd:complexContent>    
    </xsd:complexType>

<!--
 *INFORMANT DETAILS VERSION 1.1
-->
    <xsd:complexType name="Informant.V1.1.CT">
        <xsd:annotation>
            <xsd:documentation>informant version 1.1 object derives from Party v1.1 object found in common_jud.xsd</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="Party.V1.1.CT">
            </xsd:extension>
        </xsd:complexContent>    
    </xsd:complexType>
    
<!--
 *INFORMANT DETAILS VERSION 1.2
-->
    <xsd:complexType name="Informant.V1.2.CT">
        <xsd:annotation>
            <xsd:documentation>informant version 1.2 object derives from Party v1.2 object found in common_jud.xsd</xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="Party.V1.2.CT">
                <xsd:sequence>
                    <xsd:element name="StaffNumber" type="StaffNumber.V1.0.CT" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>    
    </xsd:complexType>
    
<!--
 *WITNESS DETAILS
-->
    <xsd:complexType name="Witness.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>witness object derives from Party
                               object found in common_jud.xsd, additional
                               elements include : 
                               a) Address Object
                               b) Interpret Language
                               c) Special Arrangement
                               d) Item To Be Brought
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>       
            <xsd:extension base="Party.V1.0.CT">
                <xsd:sequence>
                    <xsd:element name="Address" type="Address.V1.0.CT"/>
                    <xsd:element name="InterpretLanguage" type="Language.V1.0.CT" minOccurs="0"/>
                    <xsd:element name="SpecialArrangement" type="SpecialArrangement.V1.0.CT" minOccurs="0"/>
                    <xsd:element name="ItemToBeBrought" type="ItemToBeBrought.V1.0.CT" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

<!--
 *WITNESS DETAILS VERSION 1.1
-->
    <xsd:complexType name="Witness.V1.1.CT">
        <xsd:annotation>
            <xsd:documentation>witness version 1.1 object derives from Party.V1.1.CT
                               object found in common_jud.xsd, additional
                               elements include : 
                               a) Address Object (v1.1)
                               b) Interpret Language (v1.1)
                               c) Special Arrangement
                               d) Item To Be Brought
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>       
            <xsd:extension base="Party.V1.1.CT">
                <xsd:sequence>
                    <xsd:element name="Address" type="Address.V1.1.CT"/>
                    <xsd:element name="InterpretLanguage" type="Language.V1.1.CT" minOccurs="0"/>
                    <xsd:element name="SpecialArrangement" type="SpecialArrangement.V1.0.CT" minOccurs="0"/>
                    <xsd:element name="ItemToBeBrought" type="ItemToBeBrought.V1.0.CT" minOccurs="0"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
<!--
 *WITNESS DETAILS VERSION 1.2
-->
    <xsd:complexType name="Witness.V1.2.CT">
        <xsd:annotation>
            <xsd:documentation>witness version 1.2 object derives from Party.V1.2.CT
                               object found in common_jud.xsd, additional
                               elements include : 
                               a) Address Object (v1.2)
                               b) Interpret Language (v1.1)
                               c) Special Arrangement
                               d) Item To Be Brought
                               e) Witness Sequence Number
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>       
            <xsd:extension base="Party.V1.2.CT">
                <xsd:sequence>
                    <xsd:element name="Address" type="Address.V1.2.CT"/>
                    <xsd:element name="InterpretLanguage" type="Language.V1.1.CT" minOccurs="0"/>
                    <xsd:element name="SpecialArrangement" type="SpecialArrangement.V1.0.CT" minOccurs="0"/>
                    <xsd:element name="ItemToBeBrought" type="ItemToBeBrought.V1.0.CT" minOccurs="0"/>
                    <xsd:element name="WitnessSequenceNumber" type="WitnessSequenceNumber.V1.0.CT"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

        <xsd:complexType name="ItemToBeBrought.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>item to be brought to court</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Text.CT">
					<xsd:maxLength value="100"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>
    
<!--
 *SURETY DETAILS VERSION 1.2
-->
    <xsd:complexType name="Surety.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>surety version 1.0 object derives from Party.V1.2.CT
                               object found in common_jud.xsd, additional
                               elements include : 
                               a) Address Object (v1.2)
                               b) Surety Sequence Number
            </xsd:documentation>
        </xsd:annotation>
        <xsd:complexContent>       
            <xsd:extension base="Party.V1.2.CT">
                <xsd:sequence>
                    <xsd:element name="Address" type="Address.V1.2.CT" minOccurs="0"/>
                    <xsd:element name="SuretySequenceNumber" type="SuretySequenceNumber.V1.0.CT"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>

<!--
 *CSOD KEY
-->
    <xsd:complexType name="SODKey.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>SOD key consists of 
                               a) Chapter 
                               b) Sub Legislative
                               c) Section
                               d) Sub Section
                               e) Paragraph
                               f) Sub Paragraph
                               g) Supplement Code
                               h) Version Number
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="Chapter" type="Chapter.V1.0.CT"/>
            <xsd:element name="SubLegislative" type="SubLegislative.V1.0.CT"/>
            <xsd:element name="Section" type="Section.V1.0.CT"/>
            <xsd:element name="SubSection" type="SubSection.V1.0.CT"/>
            <xsd:element name="Paragraph" type="Paragraph.V1.0.CT"/>
            <xsd:element name="SubParagraph" type="SubParagraph.V1.0.CT"/>
            <xsd:element name="SupplementCode" type="SupplementCode.V1.0.CT"/>
            <xsd:element name="VersionNumber" type="VersionNumber.V1.0.CT"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="SODKey.V1.1.CT">
        <xsd:annotation>
            <xsd:documentation>SOD key consists of 
                               a) Chapter (v1.1)
                               b) Sub Legislative (v1.1)
                               c) Section (v1.1)
                               d) Sub Section (v1.1)
                               e) Paragraph (v1.1)
                               f) Sub Paragraph (v1.1)
                               g) Supplement Code (v1.1)
                               h) Version Number (v1.1)
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="Chapter" type="Chapter.V1.1.CT"/>
            <xsd:element name="SubLegislative" type="SubLegislative.V1.1.CT"/>
            <xsd:element name="Section" type="Section.V1.1.CT"/>
            <xsd:element name="SubSection" type="SubSection.V1.1.CT"/>
            <xsd:element name="Paragraph" type="Paragraph.V1.1.CT"/>
            <xsd:element name="SubParagraph" type="SubParagraph.V1.1.CT"/>
            <xsd:element name="SupplementCode" type="SupplementCode.V1.1.CT"/>
            <xsd:element name="VersionNumber" type="VersionNumber.V1.1.CT"/>
        </xsd:sequence>
    </xsd:complexType>

