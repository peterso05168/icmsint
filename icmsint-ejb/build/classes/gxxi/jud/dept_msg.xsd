	<xsd:complexType name="BatchSerialNumber.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>interface serial number for the version control mechanism</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Count.CT">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="9999"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="XMLRecordCount.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>number of records generated for a particular XML Message type</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Count.CT">
				<xsd:minInclusive value="0"/>
				<xsd:maxInclusive value="9999"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="OffenceDetails.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Ofference Details
	               a) Offence Date Format
	               b) Offence Date
	               c) Time Bar Method
	               d) Discover Date
	               e) Interface Offence Code
	               f) Offence Description Format
	               g) Offence Location
			</xsd:documentation>	        
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OffenceDateFormat" type="OffenceDateFormat.V1.0.CT"/>
			<xsd:element name="OffenceDateTime" type="OffenceDateTime.V1.0.CT" minOccurs="1" maxOccurs="2"/>
			<xsd:element name="TimeBarMethod" type="TimeBarMethod.V1.0.CT"/>
			<xsd:element name="DiscoverDate" type="cct:Date.CT" minOccurs="0"/>
			<xsd:element name="InterfaceOffenceCode" type="InterfaceOffenceCode.V1.0.CT" minOccurs="0"/>
			<xsd:element name="OffenceDescFormat" type="OffenceDescFormat.V1.0.CT"/>
			<xsd:element name="OffenceLocation" type="Address.V1.1.CT" minOccurs="0"/>
		</xsd:sequence>	
	</xsd:complexType>
        
        <xsd:complexType name="OffenceDetails.V1.1.CT">
		<xsd:annotation>
			<xsd:documentation>Ofference Details
	               a) Offence Date Format
	               b) Offence Date
	               c) Time Bar Method
	               d) Discover Date
	               e) Interface Offence Code
	               f) Offence Description Format
	               g) Offence Location
			</xsd:documentation>	        
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OffenceDateFormat" type="OffenceDateFormat.V1.0.CT"/>
			<xsd:element name="OffenceDateTime" type="OffenceDateTime.V1.0.CT" minOccurs="1" maxOccurs="2"/>
			<xsd:element name="TimeBarMethod" type="TimeBarMethod.V1.0.CT"/>
			<xsd:element name="DiscoverDate" type="cct:Date.CT" minOccurs="0"/>
			<xsd:element name="InterfaceOffenceCode" type="InterfaceOffenceCode.V1.0.CT" minOccurs="0"/>
			<xsd:element name="OffenceDescFormat" type="OffenceDescFormat.V1.0.CT"/>
			<xsd:element name="OffenceLocation" type="Address.V1.2.CT" minOccurs="0"/>
		</xsd:sequence>	
	</xsd:complexType>

	<xsd:complexType name="OffenceDateTime.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Offence Date and Time</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OffenceDate" type="cct:Date.CT"/>
			<xsd:element name="OffenceTime" type="cct:Time.CT"/>	
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="OffenceDateFormat.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Offence Date Format 1,2,3,4 or 5</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:maxLength value="1"/>
				<xsd:enumeration value="1"/>
				<xsd:enumeration value="2"/>
				<xsd:enumeration value="3"/>
				<xsd:enumeration value="4"/>
				<xsd:enumeration value="5"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="TimeBarMethod.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Time Bar Format 1 or 2</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:maxLength value="1"/>
				<xsd:enumeration value="1"/>
				<xsd:enumeration value="2"/>               
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="InterfaceOffenceCode.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Interface Offence Code</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="4"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="OffenceDescFormat.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Offence Description Format</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:maxLength value="1"/>
				<xsd:enumeration value="F"/>
				<xsd:enumeration value="X"/>               
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="AuthorizeUserID.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Authorize User ID</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="10"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="InputUserID.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Input User ID</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="10"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="InterfaceID.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Interface ID</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="20"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="ProcessStatus.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Process Status
            					"1" New or Modify
            					"2" Authorize
            					"3" First upload
            					"4" Download case with case no
            					"5" Down case with return code
            					"6" Reauthorize
            					"7" Reupload
            					"9" Assign case no or reject summon
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:maxLength value="1"/>
				<xsd:enumeration value="1"/>
				<xsd:enumeration value="2"/>
				<xsd:enumeration value="3"/>
				<xsd:enumeration value="4"/>
				<xsd:enumeration value="5"/>
				<xsd:enumeration value="6"/>
				<xsd:enumeration value="7"/>
				<xsd:enumeration value="9"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="HostReturnCode.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Host Return Code</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="5"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

