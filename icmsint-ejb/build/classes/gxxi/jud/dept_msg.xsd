			<xsd:element name="PGBLAllow" type="FieldIndicator.V1.0.CT" minOccurs="0"/>
			<xsd:element name="Law" type="Law.V1.0.CT"/>
			<xsd:element name="LawChinese" type="LawChinese.V1.1.CT"/>
			<xsd:element name="ShortDescription" type="ShortDescription.V1.0.CT"/>
			<xsd:element name="ShortDescriptionChinese" type="ShortDescriptionChinese.V1.1.CT"/>
			<xsd:element name="LongDescription" type="OffenceDescription.V1.0.CT"/>
			<xsd:element name="LongDescriptionChinese" type="OffenceDescriptionChinese.V1.1.CT"/>
		</xsd:sequence>
	</xsd:complexType>

