	<xsd:complexType name="InterfaceFileHeader.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>A file header indicate the details of the corresponding XML message body for easy processing	elements include :
				a) Record Prefix
				b) Serial Number
				c) Interface Date
				d) Record Count
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Prefix"				type="Prefix.V1.0.CT"/>
			<xsd:element name="BatchSerialNumber"	type="BatchSerialNumber.V1.0.CT"/>
			<xsd:element name="InterfaceDate"		type="cct:Date.CT"/> 
			<xsd:element name="RecordCount"  		type="XMLRecordCount.V1.0.CT"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="InterfaceFileHeader.V1.1.CT">
