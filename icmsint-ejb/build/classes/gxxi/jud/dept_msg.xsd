			<xsd:element name="LongDescriptionChinese" type="OffenceDescriptionChinese.V1.2.CT" minOccurs="0"/>
		</xsd:sequence>	
	</xsd:complexType>
        
        <xsd:complexType name="OffenceDesc.V1.2.CT">
		<xsd:annotation>
			<xsd:documentation>Ofference Description
	               a) PGBL Allow
	               b) Contrary to Law in English
	               c) Contrary to Law in Chinese
	               d) Offence Description in English
	               e) Offence Description in Chinse
	               f) Charge Particulars in English
	               g) Charge Particulars in Chinese
			</xsd:documentation>	        
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PGBLAllow" type="FieldIndicator.V1.0.CT" minOccurs="0"/>
                        <xsd:element name="ContraryToLaw" type="ContraryToLaw.V1.0.CT"/>
                        <xsd:element name="ContraryToLawChinese" type="ContraryToLawChinese.V1.0.CT" minOccurs="0"/>
                        <xsd:element name="OffenceDescription" type="OffenceDescription.V1.2.CT"/>
                        <xsd:element name="OffenceDescriptionChinese" type="OffenceDescriptionChinese.V1.3.CT" minOccurs="0"/>
                        <xsd:element name="ChargeParticular" type="ChargeParticular.V1.0.CT" minOccurs="0"/>
                        <xsd:element name="ChargeParticularChinese" type="ChargeParticularChinese.V1.0.CT" minOccurs="0"/>
		</xsd:sequence>	
	</xsd:complexType>

	<xsd:complexType name="OffenceVariable.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Ofference Variable		   
                           a) Offence Variable Code
                           b) Offence Variable Name/Description
                           c) Offence Variable Value
                           d) Offence Variable Value Chinese
			</xsd:documentation>	        
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="OffenceVarCode" type="VariableNumber.V1.0.CT" minOccurs="0"/>
                        <xsd:element name="OffenceVarDesc" type="VariableDescription.V1.0.CT" minOccurs="0"/>
			<xsd:element name="OffenceVarValue" type="OffenceVarValue.V1.0.CT"/>
			<xsd:element name="OffenceVarValueChinese" type="OffenceVarValueChinese.V1.0.CT" minOccurs="0"/>
		</xsd:sequence>	
	</xsd:complexType>

	<xsd:complexType name="OffenceVarValue.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Offence Variable Value</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="300"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="OffenceVarValueChinese.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Offence Variable Chinese Value</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="200"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="RecordStatus.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>record status
                               a) 'N'   - Normal
                               b) 'R'	- Rollback
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">				
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="R"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="RecordStatus.V1.1.CT">
		<xsd:annotation>
			<xsd:documentation>Appeal/ Review record status
                               a) blank - Normal entry
                               b) 'C'   - Indicates that this same entry had been sent in previous batch
                               				and it has now been cancelled due to mis-entry. If a correct 
                               				entry is re-entered by the Magistrates' Courts user, then a 
                               				new entry is to be followed.
                               c) 'R'	 - Rollback
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:enumeration value=" "/>
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="R"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
        
        <xsd:complexType name="RecordStatus.V1.2.CT">
		<xsd:annotation>
			<xsd:documentation>Appeal/ Review record status
                               a) blank - Normal entry
                               a) 'N'   - Normal entry
                               b) 'C'   - Indicates that this same entry had been sent in previous batch
                               				and it has now been cancelled due to mis-entry. If a correct 
                               				entry is re-entered by the Magistrates' Courts user, then a 
                               				new entry is to be followed.
                               c) 'R'	 - Rollback
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:enumeration value=" "/>
                                <xsd:enumeration value="N"/>
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="R"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="RecordSequenceNumber.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>interface record sequence number of this given batch</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Count.CT">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="9999"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="ApplicationStatus.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>application status for the case allocation:
                A - Accepted
                J - Rejected
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:maxLength value="1"/>
				<xsd:enumeration value="A"/>
				<xsd:enumeration value="J"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="RemarkLine.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Remark Line</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="400"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>    

	<xsd:complexType name="PartyType.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>party type</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:maxLength value="5"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="PartyNo.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>party number</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Numeric.CT">
				<xsd:totalDigits value="3"/>
				<xsd:fractionDigits value="0"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="DemandNoteNumber.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>demand note number</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="16"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="DemandNoteNumber.V1.1.CT">
		<xsd:annotation>
			<xsd:documentation>demand note number</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="25"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="CaseRowSequenceNumber.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>case row sequence number</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Count.CT">
				<xsd:minInclusive value="1"/>
				<xsd:maxInclusive value="9999"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="Amount.V1.1.CT">
		<xsd:annotation>
			<xsd:documentation>penalty amount</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Numeric.CT">
				<xsd:totalDigits value="8"/>
				<xsd:fractionDigits value="2"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="AdditionalSection.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Additional Section</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="10"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
        
        <xsd:complexType name="PreferredWarrantType.V1.0.CT">
            <xsd:annotation>
                <xsd:documentation>Preferred Warrant Type
                                                    N - Non-Payment Warrant
                                                    D - Distress Warrant
                </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleContent>
                <xsd:restriction base="cct:Code.CT">
                                    <xsd:enumeration value="N"/>
                                    <xsd:enumeration value="D"/>
                </xsd:restriction>
            </xsd:simpleContent>
        </xsd:complexType>
        
        <xsd:complexType name="FixedPenaltyApplicationNature.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Fixed Penalty Application Nature
                                                    VI - Vehicle Idling
                                                    AS - Anti-Smoking
                                                    WI - Window Inspection
                                                    PSB - Plastic Shopping Bag
                        </xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="5"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
        
            <xsd:complexType name="RecordType.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>Hearing Outcome Type
            					N - Normal
            					R - Rollback
            					C - Chang of Order Detail
			</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
				<xsd:enumeration value="N"/>
				<xsd:enumeration value="R"/>
				<xsd:enumeration value="C"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>

    <xsd:complexType name="PleaType.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>Plea type
            					1 - Plea Guilty
            					2 - Plea Not Guilty
            					3 - Plea Guilty by letter
            					4 - No Plea taken
			</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="cct:Code.CT">
				<xsd:enumeration value="1"/>
				<xsd:enumeration value="2"/>
				<xsd:enumeration value="3"/>
				<xsd:enumeration value="4"/>
            </xsd:restriction>
        </xsd:simpleContent>
    </xsd:complexType>

	<xsd:complexType name="CommonAmendment.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Basic amendment object consists of
						a) Action
						b) Data Name
						c) Data Value
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Action" type="AmendmentAction.V1.0.CT"/>
			<xsd:element name="DataName" type="AmendmentDataName.V1.0.CT"/>
			<xsd:element name="DataValue" type="AmendmentDataValue.V1.0.CT"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="AmendmentAction.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Application Action
	            	'A': Add
	            	'U': Update	            	         	
	            	'D': Delete
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:maxLength value="1"/>
				<xsd:enumeration value="A"/>
				<xsd:enumeration value="U"/>
				<xsd:enumeration value="D"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="AmendmentDataName.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Amendment Data Name</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="50"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="AmendmentDataValue.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Amendment Data Value</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="2000"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="OffenceAmendment.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Offence amendment object
						Derived from CommonAmendment.V1.0.CT
						Extension consists of
						a) Offence Sequence Number
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CommonAmendment.V1.0.CT">
				<xsd:sequence>
					<xsd:element name="OffenceSequenceNumber" type="OffenceSequenceNumber.V1.0.CT"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="DefendantAmendment.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Defendant amendment object
						Derived from CommonAmendment.V1.0.CT
						Extension consists of
						a) Defendant Sequence Number
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CommonAmendment.V1.0.CT">
				<xsd:sequence>
					<xsd:element name="DefendantSequenceNumber" type="DefendantSequenceNumber.V1.0.CT"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="WitnessAmendment.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Witness amendment object
							Derived from CommonAmendment.V1.0.CT
							Extension consists of
							a) Witness Sequence Number
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CommonAmendment.V1.0.CT">
				<xsd:sequence>
					<xsd:element name="WitnessSequenceNumber" type="WitnessSequenceNumber.V1.0.CT"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="SuretyAmendment.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Surety amendment object
							Derived from CommonAmendment.V1.0.CT
							Extension consists of
							a) Surety Sequence Number
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CommonAmendment.V1.0.CT">
				<xsd:sequence>
					<xsd:element name="SuretySequenceNumber" type="SuretySequenceNumber.V1.0.CT"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="BailRemandAmendment.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Bail Remand amendment object
							Derived from CommonAmendment.V1.0.CT
							Extension consists of
							a) Bail Remand Identifier
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CommonAmendment.V1.0.CT">
				<xsd:sequence>
					<xsd:element name="BailRemandIdentifier" type="BailRemandIdentifier.V1.0.CT"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="ChargeType.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Charge Type
	            	'P': Primary
	            	'A': Alternate	            	         	
	            	'D': Additional
                        'C': Consolidated
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:maxLength value="1"/>
				<xsd:enumeration value="P"/>
				<xsd:enumeration value="A"/>
				<xsd:enumeration value="D"/>
				<xsd:enumeration value="C"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="OffenceDateType.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Offence Date Type for Charge Case
	            	'1': On date1
	            	'2': On or about date1	            	         	
	            	'3': between date1 and date2
                        '4': on a day in year
                        '5': on a day in month and year
                        '6': others
                        '7': no time information
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:maxLength value="1"/>
				<xsd:enumeration value="1"/>
				<xsd:enumeration value="2"/>
				<xsd:enumeration value="3"/>
				<xsd:enumeration value="4"/>
				<xsd:enumeration value="5"/>
				<xsd:enumeration value="6"/>
				<xsd:enumeration value="7"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="OffenceDateFreetext.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Offence Date Free Text</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="70"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
        
        <xsd:complexType name="BailRemand.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>bail/remand details object for charge case 
                               consists of
                               a) Defendant Sequence Number
                               b) Period Type
                               c) Bail Form Reference Number (Pol.40 no.)
                               d) Surety Sequency Number
                               e) Relationship code/free text between defendant and surety
                               f) From Date
                               g) To Date
                               h) Bail Type
                               i) Cash Amount
                               j) Own/Undertaking Amount
                               k) Bail Condition
                               l) Report/Remand Location
                               m) Report Frequency
                               n) Report on Monday Indicator
                               o) Report on Tuesday Indicator
                               p) Report on Wednesday Indicator
                               q) Report on Thursday Indicator
                               r) Report on Friday Indicator
                               s) Report on Saturday Indicator
                               t) Report on Sunday Indicator
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DefendantSequenceNumber" type="DefendantSequenceNumber.V1.0.CT"/>
			<xsd:element name="PeriodType" type="PeriodType.V1.0.CT"/>
			<xsd:element name="BailFormReferenceNumber" type="BailBookSerialNumber.V1.0.CT" minOccurs="0"/>
			<xsd:element name="SuretySequenceNumber" type="SuretySequenceNumber.V1.0.CT" minOccurs="0"/>
			<xsd:element name="RelationshipCode" type="RelationshipCode.V1.0.CT" minOccurs="0"/>
                        <xsd:element name="RelationshipText" type="RelationshipText.V1.0.CT" minOccurs="0"/>
			<xsd:element name="FromDate" type="cct:Date.CT"/>
			<xsd:element name="ToDate" type="cct:Date.CT" minOccurs="0"/>
			<xsd:element name="BailType" type="BailType.V1.0.CT" minOccurs="0"/>
			<xsd:element name="CashAmount" type="Amount.V1.0.CT" minOccurs="0"/>
			<xsd:element name="OwnUndertakingAmount" type="Amount.V1.0.CT" minOccurs="0"/>
			<xsd:element name="BailCondition" type="BailCondition.V1.0.CT" minOccurs="0"/>
                        <xsd:element name="ReportRemandLocation" type="ProsecutionDepartmentStation.V1.0.CT" minOccurs="0"/>
                        <xsd:element name="ReportFrequency" type="ReportFrequency.V1.0.CT" minOccurs="0"/>
                        <xsd:element name="ReportMondayIndicator" type="FieldIndicator.V1.0.CT" minOccurs="0"/>
                        <xsd:element name="ReportTuesdayIndicator" type="FieldIndicator.V1.0.CT" minOccurs="0"/>
                        <xsd:element name="ReportWednesdayIndicator" type="FieldIndicator.V1.0.CT" minOccurs="0"/>
                        <xsd:element name="ReportThursdayIndicator" type="FieldIndicator.V1.0.CT" minOccurs="0"/>
                        <xsd:element name="ReportFridayIndicator" type="FieldIndicator.V1.0.CT" minOccurs="0"/>
                        <xsd:element name="ReportSaturdayIndicator" type="FieldIndicator.V1.0.CT" minOccurs="0"/>
                        <xsd:element name="ReportSundayIndicator" type="FieldIndicator.V1.0.CT" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="PeriodType.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Period Type
	            	'B': Bail offered
	            	'R': Remand
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:maxLength value="1"/>
				<xsd:enumeration value="B"/>
				<xsd:enumeration value="R"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="BailType.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Bail Type
	            	'A': ICAC
	            	'C': Court
                        'H': Hawker
                        'P': Police
			</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:maxLength value="1"/>
				<xsd:enumeration value="A"/>
				<xsd:enumeration value="C"/>
				<xsd:enumeration value="H"/>
				<xsd:enumeration value="P"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="RelationshipCode.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Relationship code between parties</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:maxLength value="3"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
        
        <xsd:complexType name="RelationshipText.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Relationship free text between parties</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="100"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="BailCondition.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Bail condition</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="500"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
        
        <xsd:complexType name="ReportFrequency.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Reporting frequency</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="20"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="BailRegisterNumber.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Bail register number</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:minLength value="1"/>
				<xsd:maxLength value="8"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

	<xsd:complexType name="BailBookSerialNumber.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Bail book serial number</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Code.CT">
				<xsd:minLength value="1"/>
				<xsd:maxLength value="8"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
        
<!--
 * ORDER BY CHARGE DETAILS VERSION 1.0
-->
    <xsd:complexType name="OrderByCharge.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>order details per charge version 1.0 consists of
								a) Party Type
								b) Party Sequence Number
								c) Offence Sequence Number
								d) JJO Name
								e) Plea Type
								f) Hearing Result Code
								g) Hearing Result Description
								h) Sentence Period Year
								i) Sentence Period Month
								j) Sentence Period Week
								k) Sentence Period Day
								l) Payment Order Details Object
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="PartyType" type="PartyType.V1.0.CT"/>
            <xsd:element name="PartySequenceNumber" type="BasicSequenceNumber.V1.0.CT"/>
            <xsd:element name="OffenceSequenceNumber" type="OffenceSequenceNumber.V1.0.CT"/>
            <xsd:element name="JJO" type="PartyEnglishName.V1.0.CT"/>
            <xsd:element name="PleaType" type="PleaType.V1.0.CT"/>
            <xsd:element name="HearingResultCode" type="OrderCode.V1.0.CT"/>
            <xsd:element name="SentenceType" type="OrderCode.V1.0.CT" minOccurs="0"/>
            <xsd:element name="Year" type="xsd:gYear" minOccurs="0"/>
            <xsd:element name="Month" type="xsd:gMonth" minOccurs="0"/>
            <xsd:element name="Week" type="Week.V1.0.CT" minOccurs="0"/>
            <xsd:element name="Day" type="xsd:gDay" minOccurs="0"/>
            <xsd:element name="PaymentOrder" type="PaymentOrder.V1.0.CT" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="HearingResultDescription.V1.0.CT">
		<xsd:annotation>
			<xsd:documentation>Hearing result description</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:restriction base="cct:Text.CT">
				<xsd:maxLength value="60"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>

<!--
 * ORDER BY DEFENDANT DETAILS VERSION 1.0
-->
    <xsd:complexType name="OrderByDefendant.V1.0.CT">
        <xsd:annotation>
            <xsd:documentation>consolidated order details per defendant version 1.0 consists of
								a) Defendant Sequence Number
								b) Sentence Period Year
								c) Sentence Period Month
								d) Sentence Period Week
								e) Sentence Period Day
                                                                f) Suspended Sentence Period Year
								g) Suspended Sentence Period Month
								h) Suspended Sentence Period Week
								i) Suspended Sentence Period Day
								j) Payment Order Details Object
								k) Start Date
								l) End Date
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="DefendantSequenceNumber" type="DefendantSequenceNumber.V1.0.CT"/>
            <xsd:element name="Year" type="xsd:gYear" minOccurs="0"/>
            <xsd:element name="Month" type="xsd:gMonth" minOccurs="0"/>
            <xsd:element name="Week" type="Week.V1.0.CT" minOccurs="0"/>
            <xsd:element name="Day" type="xsd:gDay" minOccurs="0"/>
            <xsd:element name="SuspendedYear" type="xsd:gYear" minOccurs="0"/>
            <xsd:element name="SuspendedMonth" type="xsd:gMonth" minOccurs="0"/>
            <xsd:element name="SuspendedWeek" type="Week.V1.0.CT" minOccurs="0"/>
            <xsd:element name="SuspendedDay" type="xsd:gDay" minOccurs="0"/>
            <xsd:element name="PaymentOrder" type="PaymentOrder.V1.0.CT" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="StartDate" type="cct:Date.CT"/>
            <xsd:element name="EndDate" type="cct:Date.CT" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    
</xsd:schema>
